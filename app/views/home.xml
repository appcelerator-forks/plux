<Alloy>
	<NavigationWindow id="navMenu" platform="ios">
		<Window id="root" title="" navBarHidden="true" class="container">
			<View id="main">
				<View id="daily_background" width="Ti.UI.FILL" height="Ti.UI.FILL"></View>
				<View layout="">
					<ImageView id="logo" borderRadius="10" width="100" height="100" top="10" left="10" image="/appicon-60@3x.png" zIndex="5" />
					<View zIndex="4" id="myinfo_view" layout="horizontal" height="Ti.UI.SIZE" top="20" backgroundColor="#B3000000"> 
						<!--<View height="40" id="fbloginView" />-->
						<View layout="horizontal" left="110" width="Ti.UI.FILL" top="10" bottom="10" height="60" id="myInfo" />
					</View>
					<View layout="vertical" id="loadingBar" height="0" width="120" borderRadius="15" top="0" opacity="0" backgroundColor="#2E2E2E" >
				        <ActivityIndicator  id="activityIndicator" />
				    	<Label top="5" text="Loading" color="#ffffff" />
				    </View>
					<ScrollView id="scrollboard" width="Titanium.UI.FILL" height="Ti.UI.FILL" zIndex="3">
						<View layout="horizontal" width="293" top="239">
							<ImageView onClick="navWindow" mod="eCard" top="15" width="139" image="/images/btn/btn_asp_e_card_pass.png"/>
							<ImageView onClick="navWindow" mod="myHealth" left="15" top="15" width="139" image="/images/btn/btn_my_health.png"/>
							<ImageView onClick="navWindow" mod="myClaim" top="15" width="139" image="/images/btn/btn_my_claim_detail.png"/>
							<ImageView onClick="navWindow" mod="profile" top="15" left="15" width="139" image="/images/btn/btn_profile.png"/>
							
							<ImageView onClick="navWindow" mod="leaflet" top="15" width="139" image="/images/btn/btn_leaflet.png"/>
							<ImageView onClick="navWindow" mod="myMedicalRecord" top="15" left="15" width="139" image="/images/btn/btn_my_medical_record.png"/>
							<ImageView onClick="navWindow" mod="clinicLocator" top="15" width="139" image="/images/btn/btn_clinic_location.png"/>
							<ImageView onClick="navWindow" mod="hra" top="15" left="15" width="139" image="/images/btn/btn_hra.png"/>
							<ImageView onClick="navWindow" mod="healthInfo" top="15" width="139" image="/images/btn/btn_healthInfo.png"/>
							
						</View>
					</ScrollView>
				</View>
			</View>
		</Window>
	</NavigationWindow>
</Alloy>